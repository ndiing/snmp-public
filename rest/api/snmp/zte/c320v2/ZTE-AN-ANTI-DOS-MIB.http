{
    "zxAnAntiDosIfConfChkInterval": 30,
    "zxAnAntiDosIfConfRateThresh": 128,
    "zxAnAntiDosIfConfRateLimit": 64,
    "zxAnAntiDosIfConfRowStatus": ""
}

